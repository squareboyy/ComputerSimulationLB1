<Window x:Class="ComputerSimulationLB1.MenuItems.ExportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComputerSimulationLB1.MenuItems"
        mc:Ignorable="d"
        Title="ExportWindow" Height="450" Width="375">
    <Grid Margin="0,10,0,0">
        <Menu Height="25" VerticalAlignment="Top" Margin="0,-5,0,0">
            <MenuItem Header="File">
                <MenuItem Header="Export" Click="ExportFile"></MenuItem>
            </MenuItem>
        </Menu>

        <DataGrid x:Name="dataGrid" IsReadOnly="True" AutoGenerateColumns="True" Margin="0,20,0,0"/>
    </Grid>
</Window>
